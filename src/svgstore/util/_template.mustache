// automatically generated from svgstore-sass task template
.svgstore {
  display: block;
  pointer-events: none;
  position: relative;
  width: 100%;
  height: 0;
  svg {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
  }
}
{{ #items }}

$svgstore--{{ name }}: {{{ data }}};

%svgstore--{{ name }} {
  background-image: {{{ data }}};
}

.svgstore--{{ name }} {
  padding-bottom: {{ height }} / {{ width }} * 100%;
}
{{ /items }}
